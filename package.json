{
  "name": "file-mapper-api",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "rest",
    "restfull",
    "api"
  ],
  "description": "",
  "author": "ozan erdogan <oerdogan90@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./dist/index.js",
    "start-dev": "nodemon -e ts -w ./src -i 'src/**/*.spec.ts' -x ts-node -- ./src/index.ts",
    "ts:watch": "tsc -w",
    "test-unit": "jest --runInBand --config=./jest.config.js",
    "test-integration": "jest --runInBand --config=./tests/jest.config.js",
    "prettier": "prettier --write --single-quote src/**/*.ts",
    "lint": "tslint -p tsconfig.json src/**/*.ts",
    "swagger": "npm run build && node ./swagger/swaggerSchemaGenerator.js"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run prettier",
      "npm run test-unit",
      "npm run test-integration",
      "npm run swagger"
    ]
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/express-correlation-id": "^1.2.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node-xlsx": "^0.12.1",
    "bcrypt": "5.0.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "connect-mongo": "2.0.3",
    "cors": "2.8.5",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-correlation-id": "^1.3.0",
    "express-fileupload": "^1.1.5",
    "express-session": "1.15.6",
    "helmet": "3.9.0",
    "joi": "14.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.7.5",
    "node-xlsx": "^0.15.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "6.0.3",
    "util": "0.11.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.16.8",
    "@types/chai": "4.1.7",
    "@types/compression": "0.0.36",
    "@types/connect-mongo": "0.0.40",
    "@types/cors": "2.8.3",
    "@types/debug": "0.0.31",
    "@types/dotenv": "6.1.0",
    "@types/ejs": "2.6.1",
    "@types/express": "4.11.0",
    "@types/express-session": "1.15.12",
    "@types/helmet": "0.0.42",
    "@types/jest": "^24.0.15",
    "@types/joi": "14.0.1",
    "@types/mongoose": "5.3.5",
    "@types/node": "^10.12.18",
    "@types/supertest": "2.0.7",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "3.0.0",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "joi-to-swagger": "^3.2.0",
    "jsdoc": "3.5.5",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.18.10",
    "prettier": "1.18.2",
    "source-map-loader": "0.2.4",
    "swagger-ui-express": "4.0.2",
    "ts-jest": "^24.0.2",
