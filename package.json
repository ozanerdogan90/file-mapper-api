{
  "name": "typescript-express-rest-api",
  "version": "2.0.6",
  "main": "./build/index.js",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "node",
    "nodejs",
    "rest",
    "restfull",
    "api",
    "builder",
    "generator"
  ],
  "description": "This generator will help you to build your own Node.js Express Mongodb API using TypeScript.",
  "author": "Checha Valerii <chechavalera@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChechaValerii/node-typescript-mongodb/issues"
  },
  "homepage": "https://github.com/ChechaValerii/node-typescript-mongodb#readme",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./dist/index.js",
    "start-dev": "nodemon -e ts -w ./src -i 'src/**/*.spec.ts' -x ts-node -- ./src/index.ts",
    "ts:watch": "tsc -w",
    "test": "NODE_ENV=test mocha -r ts-node/register test/index.js --exit",
    "prettier": "prettier --write --single-quote src/**/*.ts",
    "precommit": "lint-staged",
    "test:coverage": "nyc npm t",
    "lint": "tslint -p tsconfig.json src/**/*.ts"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "prettier --write --single-quote --fix src/**/*.ts",
      "nyc npm t"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "src/**/*model.ts",
      "src/config/*.ts",
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "bcrypt": "3.0.4",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "connect-mongo": "2.0.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "helmet": "3.9.0",
    "joi": "14.3.0",
    "mongoose": "5.4.0",
    "node-dependency-injection": "^2.3.5",
    "node-dependency-injection-express-middleware": "^1.2.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "6.0.3",
    "typedi": "^0.8.0",
    "util": "0.11.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.16.8",
    "@types/chai": "4.1.7",
    "@types/compression": "0.0.36",
    "@types/connect-mongo": "0.0.40",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.3",
    "@types/debug": "0.0.31",
    "@types/dotenv": "6.1.0",
    "@types/ejs": "2.6.1",
    "@types/express": "4.11.0",
    "@types/express-session": "1.15.12",
    "@types/helmet": "0.0.42",
    "@types/joi": "14.0.1",
    "@types/mongoose": "5.3.5",
    "@types/node": "^10.12.18",
    "@types/passport": "1.0.0",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.7",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "3.0.0",
    "@types/yeoman-generator": "3.1.0",
    "chai": "4.2.0",
    "husky": "^3.0.1",
    "jsdoc": "3.5.5",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "source-map-loader": "0.2.4",
    "supertest": "4.0.2",
    "swagger-jsdoc": "3.2.7",
    "swagger-ui-express": "4.0.2",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.8.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.4.3"
  }
}
